<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} TKD [%thread] %-5level %logger{36} - %msg%n
      </pattern>
    </encoder>
  </appender>

  <logger name="scala.slick.jdbc" level="INFO" />
  <logger name="PatientSamples" level="INFO" />

  <root level="debug">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>
